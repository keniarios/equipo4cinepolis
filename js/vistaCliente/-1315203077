cho=$(window).width(),alturaAnterior=$(".listCartelera li:first").height(),$(window).resize(function(){ValidarClicks()}),$(document).ready(function(){$(".listCartelera li.mansori .sellos").hide(),ValidarClicks()}),pausar=!1,Cartelera=[],ClaveCiudadElegida=null,$("#cmbCiudadesCartelera").html($("select[id$=cmbCiudades]").html()),$.uniform.update("#cmbCiudadesCartelera"),$(document).ready(function(){$("#cmbCiudadesCartelera").on("change",function(){$(this).find("option").index($(this).find("option:selected"))>0&&(ClaveCiudadElegida=$("#cmbCiudadesCartelera option:selected").attr("clave"),MostrarCargandoCartelera($("#cmbCiudadesCartelera").find("option:selected").text()),ObtenerCarteleraFront($("#cmbCiudadesCartelera option:selected").attr("clave")),ScrollDatalayer(".listCartelera li","Front","span.data-layer"))});BindClickVerMas();$("a[id$=lbl_encartelera]").on("click",function(n){var t,i;n.preventDefault(),t=$(this).attr("Clave"),EstaVacio(t)||(GuardarCambiarCiudad(t),EsVIP?(i=document.location.href.indexOf("/vip/")==-1?"vip/":"",document.location.href=i+"cartelera/"+t):document.location.href="cartelera/"+t)})})